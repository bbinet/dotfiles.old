[color]
  branch = auto
  diff = auto
  status = auto
  interactive = auto
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  lol = log --decorate --pretty=oneline --abbrev-commit --all --graph

[giggle]
	compact-mode = false
	main-window-maximized = false
	main-window-geometry = 1382x820+1904+136
[core]
	excludesfile = ~/.gitignore
